{
  "name": "cordova-plugin-settings-hook",
  "version": "0.2.7",
  "description": "Cordova plugin helpful to modify Android and iOS settings with config.xml parameters.\n    \tBased on the work of Devin Jett and Diego Netto on generator-ionic",
  "cordova": {
    "id": "cordova-plugin-settings-hook",
    "platforms": [
      "android",
      "ios"
    ]
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack",
    "postversion": "git push && git push --tags && npm publish"
  },
  "engines": {
    "cordovaDependencies": {
      "5.0.0": {
        "cordova-android": ">=6.3.0"
      },
      "7.0.0": {
        "cordova": ">100"
      }
    }
  },
  "bundleDependencies": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mircoc/cordova-plugin-settings-hook.git"
  },
  "keywords": [
    "cordova",
    "configuration",
    "ecosystem:cordova"
  ],
  "author": "Mirco Cipriani",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mircoc/cordova-plugin-settings-hook/issues"
  },
  "homepage": "https://github.com/mircoc/cordova-plugin-settings-hook",
  "devDependencies": {
    "cross-env": "^5.0.1",
    "webpack": "^3.0.0",
    "elementtree": "^0.1.7",
    "plist": "^2.1.0"
  }
}
